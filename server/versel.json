{
  "framework": "vercel",
  "version": 2,
  "packageManager": "npm",
  "config": {
    "runtime": "nodejs",
    "nodeVersion": "18.x"
  },
  "routes": [
    { "handle": "filesystem" },
    { "src": "/index.js", "dest": "/index.js" },
    { "src": "/dialogs_with_comments", "dest": "/index.js" }
  ]
  "builds": [
    {
      "src": "*.js",
      "use": "@vercel/node"
    }
  ]

}
